{
  "version": 2,
  "buildCommand": "pip install -r requirements-vercel.txt",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "tizzl/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "functions": {
    "api/index.py": {
      "maxDuration": 30
    }
  }
}